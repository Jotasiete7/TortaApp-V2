{
  "sqltools.connections": [
    {
      "name": "Supabase - TortaApp",
      "driver": "PostgreSQL",
      "server": "YOUR_SUPABASE_HOST.supabase.co",
      "port": 5432,
      "database": "postgres",
      "username": "postgres",
      "password": "YOUR_DATABASE_PASSWORD",
      "connectionTimeout": 30,
      "ssl": {
        "rejectUnauthorized": false
      },
      "previewLimit": 50
    }
  ],
  "sqltools.useNodeRuntime": false,
  "sqltools.autoOpenSessionFiles": false,
  "files.watcherExclude": {
    "**/node_modules/**": true,
    "**/src-tauri/target/**": true,
    "**/dist/**": true,
    "**/.git/objects/**": true,
    "**/.gemini/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/src-tauri/target": true,
    "**/dist": true,
    "**/.gemini": true
  }
}