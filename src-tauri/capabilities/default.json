{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "notification:default",
    "shell:default",
    {
      "identifier": "shell:allow-open",
      "allow": [
        {
          "href": "https://www.patreon.com/*"
        },
        {
          "href": "https://patreon.com/*"
        },
        {
          "href": "https://google.com/*"
        }
      ]
    }
  ]
}
