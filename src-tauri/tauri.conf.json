{
    "$schema": "https://schema.tauri.app/config/2",
    "productName": "TortaApp",
    "version": "2.0.0",
    "identifier": "com.tortaapp.wurm",
    "build": {
        "beforeDevCommand": "npm run dev",
        "devUrl": "http://localhost:5173",
        "beforeBuildCommand": "npm run build",
        "frontendDist": "../dist"
    },
    "bundle": {
        "active": true,
        "targets": "all",
        "icon": [
            "icons/32x32.png",
            "icons/128x128.png",
            "icons/128x128@2x.png",
            "icons/icon.icns",
            "icons/icon.ico"
        ]
    },
    "app": {
        "windows": [
            {
                "fullscreen": false,
                "resizable": true,
                "title": "TortaApp - Wurm Trade Analytics",
                "width": 1400,
                "height": 900,
                "minWidth": 1200,
                "minHeight": 700,
                "center": true
            }
        ],
        "security": {
            "csp": null
        }
    },
    "plugins": {}
}
